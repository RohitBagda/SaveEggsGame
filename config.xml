<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.macalester.savetheeggs" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Save The Eggs</name>
    <description>
        INSERT DESCRIPTION HERE
    </description>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <icon src="res/icon/ios/icon-1024-noalpha.png" width="1024" height="1024" />
        <icon src="res/icon/ios/icon-72-2x.png" width="144" height="144" />
        <icon src="res/icon/ios/icon-72.png" width="72" height="72" />
        <icon src="res/icon/ios/icon-57-2x.png" width="114" height="114" />
        <icon src="res/icon/ios/icon-57.png" width="57" height="57" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />

    <plugin name="cordova-plugin-browsersync" spec="~0.1.7" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.1.1" />
</widget>
