<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.macalester.savetheeggs" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Save The Eggs</name>
    <description>
        Fun cross-platform mobile game
    </description>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="www/assets/images/app_icon.png" />
    </platform>
    <platform name="ios">
        <icon src="res/icon/ios/icon-1024-noalpha.png" width="1024" height="1024" />
        <icon src="res/icon/ios/icon-29.png" width="29" height="29" />
        <icon src="res/icon/ios/icon-57.png" width="57" height="57" />
        <icon src="res/icon/ios/icon-58.png" width="58" height="58" />
        <icon src="res/icon/ios/icon-80.png" width="80" height="80" />
        <icon src="res/icon/ios/icon-87.png" width="87" height="87" />
        <icon src="res/icon/ios/icon-114.png" width="114" height="114" />
        <icon src="res/icon/ios/icon-120.png" width="120" height="120" />
        <icon src="res/icon/ios/icon-180.png" width="180" height="180" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
    <plugin name="cordova-plugin-browsersync" spec="~0.1.7" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.1.1" />
</widget>
